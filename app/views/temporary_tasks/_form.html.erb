<%= simple_form_for(@temporary_task, url: list_temporary_task_path ) do |f| %>
  <%= f.error_notification %>
<br>
  <div class="form-inputs">
    <%= f.input :description, label: 'Descripcion' %>
    <%= f.input :state, collection: states, label: "Estado" %>
    <%= f.input :priority, collection: priorities, label: "Prioridad" %>
    <%= f.input :start, label:"Comienza" %>
    <%= f.input :finish, laber:"Finaliza" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<br>
<% end %>
