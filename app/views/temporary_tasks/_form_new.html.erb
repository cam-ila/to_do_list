<%= simple_form_for(@temporary_task, url: { :action => :create })do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :description, required: true, label: 'Descripcion' %>
    <%= f.input :state, collection: states, selected: states.first, label: "Estado"  %>
    <%= f.input :priority, collection: priorities, selected: priorities.first, label:"Prioridad" %>
    <%= f.input :start, label: "Comienza" %>
    <%= f.input :finish, label: "Finaliza" %>
  </div>
<p>
  
</p>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

